{
    "version": 2,
    "functions": {
        "api/index.ts": {
            "runtime": "@vercel/node@3",
            "maxDuration": 30
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.ts"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}