{
    "version": 2,
    "functions": {
        "api/index.mjs": {
            "maxDuration": 30
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.mjs"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}