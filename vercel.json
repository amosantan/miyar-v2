{
    "version": 2,
    "functions": {
        "api/**": {
            "maxDuration": 300
        }
    },
    "crons": [
        {
            "path": "/api/cron/ingestion",
            "schedule": "0 6 * * 1"
        }
    ],
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.js"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}